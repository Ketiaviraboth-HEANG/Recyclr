generator client {
  provider        = "prisma-client-py"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DB_URL")
  extensions = [vector]
}

model product {
  id        String                   @id @default(uuid())
  name      String
  embedding Unsupported("vector(4)")
}
